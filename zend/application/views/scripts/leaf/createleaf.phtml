function createleaf(){
 	var type=$('input[name=type]:checked').val();
	var leafname=document.getElementById('leafname').value.trim();
	var category=document.getElementById('category').value.trim();
	var subcategory=document.getElementById('subcategory').value.trim();
	if(leafname!='' && type!='' && category!='' && subcategory!=''){
		document.getElementById('light').style.display='block';
   	  document.getElementById('fade').style.display='block';
 	var parameters="type="+type+"&leafname="+leafname+"&category="+category+"&subcategory="+subcategory;
	 $.ajax({
		    url:'http://www.freniz.com/leaf/create',
		    cache:false,
		    data:parameters,
		    dataType:"json",
		    success:function(json){
		    if(json.status=='success'){
		    	window.location.href="http://www.freniz.com/leafedit/"+json.leafid;
		    	}else 
		    	  alert('Error while creating this leaf! please try again later');
		    }
	    } );
	    }else{
	    		alert("please don't be empty");
	    }
}
